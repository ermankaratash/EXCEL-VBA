Private Sub CommandButton8_Click()
  UserForm2.Hide
End Sub

Private Sub UserForm_Initialize()
  RENK1.Value = Worksheets("DATA SHEET").Range("K2").Value
  RENK2.Value = Worksheets("DATA SHEET").Range("K3").Value
  RENK3.Value = Worksheets("DATA SHEET").Range("K4").Value
  RENK4.Value = Worksheets("DATA SHEET").Range("K5").Value
  RENK5.Value = Worksheets("DATA SHEET").Range("K6").Value
  RENK6.Value = Worksheets("DATA SHEET").Range("K7").Value
  RENK7.Value = Worksheets("DATA SHEET").Range("K8").Value
  RENK8.Value = Worksheets("DATA SHEET").Range("K9").Value
  RENK9.Value = Worksheets("DATA SHEET").Range("K10").Value
  RENK10.Value = Worksheets("DATA SHEET").Range("K11").Value
  RENK11.Value = Worksheets("DATA SHEET").Range("K12").Value
  RENK12.Value = Worksheets("DATA SHEET").Range("K13").Value
  RENK13.Value = Worksheets("DATA SHEET").Range("K14").Value
  RENK14.Value = Worksheets("DATA SHEET").Range("K15").Value
  RENK15.Value = Worksheets("DATA SHEET").Range("K16").Value
End Sub

Private Sub UserForm_QueryClose(Cancel As Integer, CloseMode As Integer)
  If CloseMode = 0 Then 
    Cancel = True
End Sub
