Private Sub SATIR21()
  Dim Bul2 As Range
  Set Bul2 = Sheets("config list").Columns(2).Find(what:=UserForm2.RENK1.Value, LookIn:=xlValues, LookAt:=xlWhole, SearchOrder:=xlByRows, SearchDirection:=xlNext, MatchCase:=False)
  If UserForm2.CheckBox1.Value = False Or EDP1.Value = "" Or REVENUE.Value = "" Or ComboBox3.Value = "" Or (CheckBox1.Value = True And SERIAL1.Value = "") Then
    WriteRow = WorksheetFunction.CountA(Range("A:A")) + 1
    Cells(WriteRow, 1).Value = ""
    Cells(WriteRow, 2).Value = ""
    Cells(WriteRow, 3).Value = ""
    Cells(WriteRow, 4).Value = ""
    Cells(WriteRow, 5).Value = ""
    Cells(WriteRow, 6).Value = ""
    Cells(WriteRow, 7).Value = ""
    Cells(WriteRow, 8).Value = ""
    Cells(WriteRow, 9).Value = ""
    Cells(WriteRow, 10).Value = ""
    Cells(WriteRow, 11).Value = ""
    Cells(WriteRow, 12).Value = ""
    Cells(WriteRow, 13).Value = ""
    Cells(WriteRow, 14).Value = ""
    Cells(WriteRow, 15).Value = ""
    Cells(WriteRow, 16).Value = ""
  Else
    WriteRow = WorksheetFunction.CountA(Range("A:A")) + 1
    Cells(WriteRow, 1).Value = ComboBox1.Value
    Cells(WriteRow, 2).Value = ComboBox4.Value
    Cells(WriteRow, 3).Value = TextBox1.Value
    Cells(WriteRow, 8).Value = ComboBox3.Value
    Cells(WriteRow, 9).Value = EIBM.Value
    Cells(WriteRow, 10).Value = EIEM.Value
    Cells(WriteRow, 11).Value = CSD.Value
    Cells(WriteRow, 12).Value = TD.Value
    Cells(WriteRow, 13).Value = EOEM.Value
    Cells(WriteRow, 14).Value = ONO.Value
    Cells(WriteRow, 15).Value = IM.Value
    Cells(WriteRow, 4).Value = Bul2.Offset(0, 2).Value
    Cells(WriteRow, 5).Value = Bul2.Offset(0, 1).Value
    Cells(WriteRow, 6).Value = Bul2.Offset(0, 3).Value

    WriteRow = WorksheetFunction.CountA(Range("A:A")) + 1
    Cells(WriteRow, 1).Value = ComboBox1.Value
    Cells(WriteRow, 2).Value = ComboBox4.Value
    Cells(WriteRow, 3).Value = TextBox1.Value
    Cells(WriteRow, 8).Value = ComboBox3.Value
    Cells(WriteRow, 9).Value = EIBM.Value
    Cells(WriteRow, 10).Value = EIEM.Value
    Cells(WriteRow, 11).Value = CSD.Value
    Cells(WriteRow, 12).Value = TD.Value
    Cells(WriteRow, 13).Value = EOEM.Value
    Cells(WriteRow, 14).Value = ONO.Value
    Cells(WriteRow, 15).Value = IM.Value
    Cells(WriteRow, 4).Value = Bul2.Offset(1, 2).Value
    Cells(WriteRow, 5).Value = Bul2.Offset(1, 1).Value
    Cells(WriteRow, 6).Value = Bul2.Offset(1, 3).Value

    WriteRow = WorksheetFunction.CountA(Range("A:A")) + 1
    Cells(WriteRow, 1).Value = ComboBox1.Value
    Cells(WriteRow, 2).Value = ComboBox4.Value
    Cells(WriteRow, 3).Value = TextBox1.Value
    Cells(WriteRow, 8).Value = ComboBox3.Value
    Cells(WriteRow, 9).Value = EIBM.Value
    Cells(WriteRow, 10).Value = EIEM.Value
    Cells(WriteRow, 11).Value = CSD.Value
    Cells(WriteRow, 12).Value = TD.Value
    Cells(WriteRow, 13).Value = EOEM.Value
    Cells(WriteRow, 14).Value = ONO.Value
    Cells(WriteRow, 15).Value = IM.Value
    Cells(WriteRow, 4).Value = Bul2.Offset(2, 2).Value
    Cells(WriteRow, 5).Value = Bul2.Offset(2, 1).Value
    Cells(WriteRow, 6).Value = Bul2.Offset(2, 3).Value
  End If
End Sub

