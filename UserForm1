    UNIT14.Text = Bul.Offset(13, 3).Value
    UNIT15.Text = Bul.Offset(14, 3).Value
    UNIT16.Text = Bul.Offset(15, 3).Value
    UNIT17.Text = Bul.Offset(16, 3).Value
    UNIT18.Text = Bul.Offset(17, 3).Value
    UNIT19.Text = Bul.Offset(18, 3).Value
    UNIT20.Text = Bul.Offset(19, 3).Value
  End If

End Sub

Private Sub CommandButton1_Click()
  Worksheets("Current Forecast").Activate   'Current Forecast is the Excel Sheet name and it is activated by this command

  Dim WriteRow As Long                      
  Dim Ctrl As MSForms.Control
  Dim N As Long
  N = 0
  For Each Ctrl In UserForm2.Controls        'We are counting checkboxes that are not checked
    If TypeOf Ctrl Is MSForms.CheckBox Then
      If Ctrl = False Then
        N = N + 1
      End If
    End If
  Next Ctrl

  If REVENUE.Value = "" Then                 'These are the controls before adding a record
    MsgBox "Please Enter a REVENUE"
  ElseIf ComboBox3.Value = "" Then
    MsgBox "Please Enter %WIN "
  ElseIf CheckBox1.Value = True And SERIAL1.Value = "" Then
    MsgBox "Please Enter SERIAL NUMBER "
  ElseIf EDP1.Value = "" Then
    MsgBox "Please Enter PRODUCTS "
  ElseIf ComboBox2.Value Like "*X" And N = 15 Then        'This is the checkbox that are not checked control
    MsgBox "Please Enter 5TH COLOUR "
    Exit Sub
  End If
 
  Call SATIR1
  Call SATIR2
  Call SATIR3
  Call SATIR4
  Call SATIR5
  Call SATIR6
  Call SATIR7
  Call SATIR8
  Call SATIR9
  Call SATIR10
  Call SATIR11
  Call SATIR12
  Call SATIR13
  Call SATIR14
  Call SATIR15
  Call SATIR16
  Call SATIR17
  Call SATIR18
  Call SATIR19
  Call SATIR20
  Call SATIR21
  Call SATIR22
  Call SATIR23
  Call SATIR24
  Call SATIR25
  Call SATIR26
  Call SATIR27
  Call SATIR28
  Call SATIR29
  Call SATIR30
  Call SATIR31
  Call SATIR32
  Call SATIR33
  Call SATIR34
  Call SATIR35

  Unload Me
  UserForm1.Show
  Unload UserForm2

End Sub
