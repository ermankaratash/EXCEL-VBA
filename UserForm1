Private Sub CommandButton3_Click()

  Dim Bul As Range
  If ComboBox2.Value <> "" Then
    Set Bul = Sheets("config list").Columns(2).Find(what:=ComboBox2.Value, LookIn:=xlValues, LookAt:=xlWhole, SearchOrder:=xlByRows, SearchDirection:=xlNext, MatchCase:=False)
    EDP1.Text = Bul.Offset(0, 2).Value
    EDP2.Text = Bul.Offset(1, 2).Value
    EDP3.Text = Bul.Offset(2, 2).Value
    EDP4.Text = Bul.Offset(3, 2).Value
    EDP5.Text = Bul.Offset(4, 2).Value
    EDP6.Text = Bul.Offset(5, 2).Value
    EDP7.Text = Bul.Offset(6, 2).Value
    EDP8.Text = Bul.Offset(7, 2).Value
    EDP9.Text = Bul.Offset(8, 2).Value
    EDP10.Text = Bul.Offset(9, 2).Value
    EDP11.Text = Bul.Offset(10, 2).Value
    EDP12.Text = Bul.Offset(11, 2).Value
    EDP13.Text = Bul.Offset(12, 2).Value
    EDP14.Text = Bul.Offset(13, 2).Value
    EDP15.Text = Bul.Offset(14, 2).Value
    EDP16.Text = Bul.Offset(15, 2).Value
    EDP17.Text = Bul.Offset(16, 2).Value
    EDP18.Text = Bul.Offset(17, 2).Value
    EDP19.Text = Bul.Offset(18, 2).Value
    EDP20.Text = Bul.Offset(19, 2).Value
    MODEL1.Text = Bul.Offset(0, 1).Value
    MODEL2.Text = Bul.Offset(1, 1).Value
    MODEL3.Text = Bul.Offset(2, 1).Value
    MODEL4.Text = Bul.Offset(3, 1).Value
    MODEL5.Text = Bul.Offset(4, 1).Value
    MODEL6.Text = Bul.Offset(5, 1).Value
    MODEL7.Text = Bul.Offset(6, 1).Value
    MODEL8.Text = Bul.Offset(7, 1).Value
    MODEL9.Text = Bul.Offset(8, 1).Value
    MODEL10.Text = Bul.Offset(9, 1).Value
    MODEL11.Text = Bul.Offset(10, 1).Value
    MODEL12.Text = Bul.Offset(11, 1).Value
    MODEL13.Text = Bul.Offset(12, 1).Value
    MODEL14.Text = Bul.Offset(13, 1).Value
    MODEL15.Text = Bul.Offset(14, 1).Value
    MODEL16.Text = Bul.Offset(15, 1).Value
    MODEL17.Text = Bul.Offset(16, 1).Value
    MODEL18.Text = Bul.Offset(17, 1).Value
    MODEL19.Text = Bul.Offset(18, 1).Value
    MODEL20.Text = Bul.Offset(19, 1).Value
    UNIT1.Text = Bul.Offset(0, 3).Value
    UNIT2.Text = Bul.Offset(1, 3).Value
    UNIT3.Text = Bul.Offset(2, 3).Value
    UNIT4.Text = Bul.Offset(3, 3).Value
    UNIT5.Text = Bul.Offset(4, 3).Value
    UNIT6.Text = Bul.Offset(5, 3).Value
    UNIT7.Text = Bul.Offset(6, 3).Value
