Private Sub ComboBox1_DropButtonClick()
  ComboBox1.List = Worksheets("DATA SHEET").Range("A2:A4").Value
End Sub

Private Sub ComboBox2_DropButtonClick()
  ComboBox2.List = Worksheets("DATA SHEET").Range("G2:G38").Value
End Sub

Private Sub ComboBox3_DropButtonClick()
  ComboBox3.List = Worksheets("DATA SHEET").Range("C2:C7").Value
End Sub

Private Sub ComboBox4_DropButtonClick()
  ComboBox4.List = Worksheets("DATA SHEET").Range("I2:I15").Value
End Sub

Private Sub CommandButton4_Click()
  UserForm2.Show
End Sub

Private Sub EIBM_DropButtonClick()
  EIBM.List = Worksheets("DATA SHEET").Range("e2:e13").Value
End Sub

